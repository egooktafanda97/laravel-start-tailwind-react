        "cross-env": "^7.0.0",
        "laravel-mix": "^5.0.1",
        "laravel-mix-purgecss": "^5.0.0-rc.1",
        "lodash": "^4.17.15",
        "postcss-import": "^12.0.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "tailwindcss": "^1.2.0"
    },
